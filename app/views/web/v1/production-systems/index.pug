extends ./../../../layouts/default

include ./../../../shared/page_components

block content
  .container
    +render_back_to_link('Home Page', '/portal')

    .row.my-2
      .col-12
        h1 Heraeus Assembly Material Production Control System

    .card.my-4
      .card-body
        .row
          .col-md-6.col-sm-12.my-3
            img.img-fluid(src="/public/images/production-systems/assembly-materials.png")
          .col-md-6.col-sm-12.my-3
            .row
              .col-12.my-2
                a.btn.btn-primary.btn-block(href="/ps/solder-wires") Solder Wires
              .col-12.my-2
                a.btn.btn-primary.btn-block(href="/ps/solder-pastes") Solder Pastes
